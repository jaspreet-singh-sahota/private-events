<!DOCTYPE html>
<html>
  <head>
    <title>Events Management</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

   <%= render 'header_nav'%>
    <% flash.each do |key , msg|%>
      <%= msg %>
    <% end %>

    <%= yield %>
  </body>
</html>
